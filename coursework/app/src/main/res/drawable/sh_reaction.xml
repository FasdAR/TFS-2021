<?xml version="1.0" encoding="utf-8"?>
<ripple
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/black_800">

    <item android:id="@+id/mask">
        <shape>
            <solid android:color="#fff"/>
            <corners android:radius="10.0dp" />
        </shape>
    </item>
    <item>
        <selector>
            <item android:state_selected="true" android:drawable="@drawable/sh_reaction_selected"/>
            <item android:drawable="@drawable/sh_reaction_unselected"/>
        </selector>
    </item>
</ripple>